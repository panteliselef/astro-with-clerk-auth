{
  "name": "astro-clerk-auth",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "postbuild": "rm -rf ./astro-components && mkdir ./astro-components/  && cp  -r ./src/astro-components/* ./astro-components/",
    "lint": "eslint \"**/*.ts*\""
  },
  "files": [
    "dist",
    "client",
    "server",
    "stores",
    "astro-components",
    "client.d.ts",
    "env.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client/react": {
      "types": "./dist/client/react/index.d.ts",
      "default": "./dist/client/react/index.js"
    },
    "./stores": {
      "types": "./dist/stores/index.d.ts",
      "default": "./dist/stores/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./client": "./client.d.ts",
    "./env": "./env.d.ts",
    "./astro-components": "./astro-components/index.ts",
    "./astro-components/*": "./astro-components/*",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@nanostores/react": "0.7.1",
    "nanostores": "^0.9.5"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "react": "^18.2.0",
    "tsconfig": "*",
    "typescript": "^4.5.2",
    "tsup": "^6.7.0"
  },
  "peerDependencies": {
    "@clerk/backend": "1.0.0-alpha-v5.2",
    "@clerk/clerk-js": "5.0.0-alpha-v5.2",
    "@clerk/shared": "^2.0.0-alpha-v5.2",
    "@clerk/types": "^4.0.0-alpha-v5.2",
    "astro": "^3.2.0"
  }
}
